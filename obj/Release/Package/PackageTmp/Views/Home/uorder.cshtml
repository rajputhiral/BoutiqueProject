@model List<BoutiqueProject.Models.OrderTbModel>
@{
    ViewBag.Title = "uorder";
    Layout = "~/Views/Shared/UserMaster.cshtml";
}


<div class="fashion_section">
    <div id="main_slider" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <div class="container">
                    <h1 class="fashion_taital">Order</h1>
                    <div class="text-center">
                        <div class="row  text-center">


                            <div class="col-lg-12 col-sm-12">
                                <table class="table table-bordered">
                                    <thead>
                                        <tr>
                                            <th>Boutique Name</th>
                                            <th>Product Name</th>
                                            <th>Product Price</th>
                                            <th>Quantity</th>
                                            <th>Price</th>
                                            <th>Ordered Date</th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model)
                                        {
                                            <tr>
                                                <td>@item.bname</td>
                                                <td>@item.pname</td>
                                                <td>@item.price</td>
                                                <td>@item.quantity</td>
                                                <td>@item.price</td>
                                                <td>@item.odate</td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>

                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>





