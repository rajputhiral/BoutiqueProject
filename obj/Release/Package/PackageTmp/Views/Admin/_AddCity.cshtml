@model BoutiqueProject.Models.City



<div class="form-group">
    <label>Country Name</label>
    @Html.DropDownListFor(m => m.cid, new SelectList(ViewBag.country, "cid", "cntrynm"), "--Select--", new { @class = "form-control" })

</div>

<div class="form-group">
    <label>State Name</label>
    @Html.DropDownListFor(m => m.stid, new SelectList(ViewBag.state, "stid", "stname"), "--Select--", new { @class = "form-control" })

</div>

<div class="form-group">
    <label>City Name</label>
    @Html.TextBoxFor(m => m.citynm, new { @class = "form-control" })

</div>

