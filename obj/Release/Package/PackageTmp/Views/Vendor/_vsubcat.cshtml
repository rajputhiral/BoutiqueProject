@model BoutiqueProject.Models.SubCategory


<div class="form-group">
    <label>Category Name</label>
    @Html.DropDownListFor(m => m.catid, new SelectList(ViewBag.cat, "catid", "catnm"), "--Select--", new { @class = "form-control" })

</div>

<div class="form-group">
    <label>Sub Category Name</label>
    @Html.TextBoxFor(m => m.subcatname, new { @class = "form-control" })

</div>

<div class="form-group">
    <label>Category Image</label>
    @Html.TextBoxFor(m => m.subcatpic, new { @type = "file" })

</div>

<div class="form-group">
    <input type="submit" class="btn btn-primary" value="Save" />
    <input type="reset" class="btn btn-primary" value="Cancel" />
</div>



